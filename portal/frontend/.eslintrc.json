{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser":"@babel/eslint-parser",
  "env": {
    "node": true
  },
  "rules": {
    "semi": [2, "never"],
    "quotes": [2, "single", { "avoidEscape": true }],
    "jsx-quotes": [2, "prefer-single"],
    "react/jsx-uses-react": "off",
    "react/prop-types": "off",
    "react/react-in-jsx-scope": "off"
  },
  "globals": {
    "React": "writable"
  }
}
